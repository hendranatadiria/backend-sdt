{
  "name": "@hendranatadiria/backend-sdt",
  "version": "1.0.0",
  "description": "Backend Assessment Test of SDT",
  "main": "dist/index.js",
  "repository": "github.com/hendranatadiria/backend-sdt",
  "author": "Bernardinus Hendra Natadiria",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently -n api,worker \"yarn run run-dev\" \"yarn run run-worker\"",
    "run-dev": "nodemon --exec ts-node --files src/index.ts",
    "run-worker": "nodemon --exec ts-node --files src/worker.ts",
    "build": "tsc",
    "start-all": "yarn start & yarn start-worker",
    "start": "node dist/index.js",
    "start-worker": "node dist/worker.js",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/luxon": "^3.3.2",
    "@types/node-cron": "^3.0.9",
    "@types/supertest": "^2.0.14",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "bullmq": "^4.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "luxon": "^3.4.3",
    "node-cron": "^3.0.2",
    "prisma": "^5.4.1",
    "yup": "^1.3.2"
  }
}
